{
    "name": "philipnjuguna/menu-generator",
    "description": "A simple package to generate sidebar for admin dashboard, preset for adminLte",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Philip Njuguna",
            "email": "philipnjuguna66@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "PhilipNjuguna\\MenuGenerator\\" : "src/"
        }
    },
    "extra": {
